#include<stdio.h>
inr main()
{
  int rows;
  printf("Enter the Number of rows");
  scanf("%d",&rows);
  for(int i=1;i<=rows;i++)
 {
    for(int j=1;j<=i;j++)
    {
    for(int space=1;space<=rows;space++)
  {
      printf("");
  } 
    for(int j=1;j<=i;j++)
      {
      printf ("%d",i);
    }
      printf ("\n");
 }
    return 0;
 }


  
  
  
  return 0;
  }
